void print(int number) {
	int last_num = number % 10;
	if (last_num == 5 || last_num == 6 || last_num == 7 || last_num == 8 || last_num == 9 || last_num == 0) {
		std::cout << number << " компьютеров";
	}
	else if (last_num == 2 || last_num == 3 || last_num == 4 ) {
		std::cout << number << " компьютера";
	}
	else {
		std::cout << number << " компьютер";
	}	
}
